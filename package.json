{
  "name": "kyun-electron-hosts",
  "productName": "kyun-electron-hosts",
  "version": "1.0.6",
  "description": "kyun-electron-hosts",
  "main": "./dist/electron.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KaiserY/kyun-electron-hosts.git"
  },
  "keywords": [
    "Angular"
  ],
  "author": "KaiserY <yueyang.fanqiang@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KaiserY/kyun-electron-hosts/issues"
  },
  "homepage": "https://github.com/KaiserYkyun-electron-hosts#readme",
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "webdriver-manager": "webdriver-manager",
    "protractor": "protractor",
    "clean": "rimraf dist",
    "start": "npm run build:dev && concurrently \"better-npm-run server:dev\" \"electron ./\"",
    "electron": "electron ./",
    "electron:dev": "npm run build:dev && electron ./",
    "electron:prod": "npm run build:prod && electron ./",
    "test": "karma start",
    "server:dev": "npm run clean && better-npm-run server:dev",
    "server:prod": "npm run clean && better-npm-run server:prod",
    "build:dev": "npm run clean && better-npm-run build:dev",
    "build:prod": "npm run clean && better-npm-run build:prod",
    "pack": "npm run clean && npm run build:dev && build --dir",
    "dist": "npm run clean && npm run build:prod && build",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor"
  },
  "betterScripts": {
    "build:dev": {
      "command": "webpack --progress --profile --colors --display-reasons --display-error-details",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "build:prod": {
      "command": "webpack --progress --profile --colors --display-reasons --display-error-details",
      "env": {
        "NODE_ENV": "prod"
      }
    },
    "server:dev": {
      "command": "webpack --progress --profile --colors --display-reasons --display-error-details --watch",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "server:prod": {
      "command": "webpack --progress --profile --colors --display-reasons --display-error-details --watch",
      "env": {
        "NODE_ENV": "prod"
      }
    }
  },
  "build": {
    "appId": "kyun.electron.hosts",
    "files": [
      "dist/**/*"
    ],
    "linux": {
      "target": "rpm",
      "depends": []
    },
    "win": {
      "msi": false
    }
  },
  "directories": {
    "output": "./dist/app"
  },
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "2.4.2",
    "@angular/compiler": "2.4.2",
    "@angular/core": "2.4.2",
    "@angular/forms": "2.4.2",
    "@angular/http": "2.4.2",
    "@angular/material": "2.0.0-beta.1",
    "@angular/platform-browser": "2.4.2",
    "@angular/platform-browser-dynamic": "2.4.2",
    "@angular/router": "3.4.2",
    "@angular/upgrade": "2.4.2",
    "@types/codemirror": "0.0.37",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.40",
    "@types/node": "^6.0.58",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "^2.1.0",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "better-npm-run": "0.0.14",
    "codelyzer": "2.0.0-beta.4",
    "codemirror": "^5.22.0",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.26.1",
    "devtron": "^1.4.0",
    "electron": "^1.4.13",
    "electron-builder": "^11.2.0",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "istanbul-instrumenter-loader": "^1.2.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^2.1.5",
    "karma-webpack": "1.8.1",
    "material-design-icons": "^3.0.1",
    "postcss-loader": "^1.2.1",
    "protractor": "^4.0.14",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "roboto-fontface": "^0.6.0",
    "rxjs": "5.0.3",
    "script-ext-html-webpack-plugin": "^1.4.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.1",
    "sudo-prompt": "^6.2.1",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "ts-node": "^2.0.0",
    "tslint": "^4.3.1",
    "tslint-loader": "^3.3.0",
    "typedoc": "^0.5.3",
    "typescript": "^2.1.4",
    "webpack": "^2.2.0-rc.2",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "2.3.1",
    "webpack-node-externals": "^1.5.4",
    "zone.js": "0.7.4"
  }
}
