{
  "name": "kyun-client-web",
  "version": "1.0.0",
  "description": "kyun-client-web",
  "main": "dist/electron.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KaiserY/kyun-client-web.git"
  },
  "keywords": [
    "Angular"
  ],
  "author": "KaiserY <yueyang.fanqiang@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KaiserY/kyun-client-web/issues"
  },
  "homepage": "https://github.com/KaiserY/kyun-client-web#readme",
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "webdriver-manager": "webdriver-manager",
    "protractor": "protractor",
    "clean": "rimraf dist",
    "start": "npm run server:dev",
    "electron:dev": "npm run build:dev && electron ./",
    "electron:prod": "npm run build:prod && electron ./",
    "test": "karma start",
    "server:dev": "npm run clean && better-npm-run server:dev",
    "server:prod": "npm run clean && better-npm-run server:prod",
    "build:dev": "npm run clean && better-npm-run build:dev",
    "build:prod": "npm run clean && better-npm-run build:prod",
    "pack": "npm run clean:dist && npm run build:prod && build",
    "dist": "npm run clean:dist && npm run build:prod && build",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor"
  },
  "betterScripts": {
    "build:dev": {
      "command": "webpack --progress --profile --colors --display-reasons --display-error-details",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "build:prod": {
      "command": "webpack --progress --profile --colors --display-reasons --display-error-details",
      "env": {
        "NODE_ENV": "prod"
      }
    },
    "server:dev": {
      "command": " webpack --progress --profile --colors --display-reasons --display-error-details --watch",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "server:prod": {
      "command": " webpack --progress --profile --colors --display-reasons --display-error-details --watch",
      "env": {
        "NODE_ENV": "prod"
      }
    }
  },
  "build": {
    "download": {
      "mirror": "https://npm.taobao.org/mirrors/electron/",
      "strictSSL": false
    },
    "linux": {
      "target": "rpm",
      "depends": []
    },
    "win": {
      "msi": false
    }
  },
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "^2.2.1",
    "@angular/http": "2.2.1",
    "@angular/material": "^2.0.0-alpha.9-3",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/router": "3.2.1",
    "@angular/upgrade": "^2.2.1",
    "core-js": "^2.4.1",
    "roboto-fontface": "^0.6.0",
    "rxjs": "5.0.0-rc.4",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^6.0.48",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "^2.0.0",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.3",
    "better-npm-run": "0.0.13",
    "browser-sync": "^2.18.2",
    "browser-sync-webpack-plugin": "^1.1.3",
    "codelyzer": "1.0.0-beta.4",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.0",
    "css-to-string-loader": "^0.1.2",
    "devtron": "^1.4.0",
    "electron": "^1.4.7",
    "electron-builder": "^8.6.0",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.1",
    "karma-remap-coverage": "^0.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "node-sass": "^3.13.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.13",
    "postcss-loader": "^1.1.1",
    "protractor": "^4.0.11",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "script-ext-html-webpack-plugin": "^1.3.4",
    "style-loader": "^0.13.1",
    "stylelint": "^7.6.0",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "ts-node": "^1.7.0",
    "tslint": "^4.0.1",
    "tslint-loader": "^2.1.5",
    "typedoc": "^0.5.1",
    "typescript": "^2.0.10",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^0.17.0"
  }
}
