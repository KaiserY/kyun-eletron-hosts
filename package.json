    "style-loader": "0.16.0",
    "stylelint": "7.9.0",
    "sudo-prompt": "7.0.0",
    "to-string-loader": "1.1.5",
    "ts-helpers": "1.1.2",
    "ts-node": "3.0.2",
    "tslint": "4.5.1",
    "tslint-loader": "3.4.3",
    "typedoc": "0.5.9",
    "typescript": "2.2.2",
    "webpack": "2.3.2",
    "webpack-dev-middleware": "1.10.1",
    "webpack-md5-hash": "0.0.5",
